<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.1.xsd">

  <module name="stock" package="com.axelor.apps.stock.db"/>

  <entity name="StockHistoryLine" persistable="false">
    <integer name="countIncMvtStockPeriod" readonly="true" title="Nb inc. moves period"/>
    <decimal name="sumIncQtyPeriod" readonly="true" title="Qty inc. moves period"/>
    <decimal name="priceIncStockMovePeriod" readonly="true" title="Amount inc. moves period"/>
    <integer name="countOutMvtStockPeriod" readonly="true" title="Nb out. moves period"/>
    <decimal name="sumOutQtyPeriod" readonly="true" title="Qty out. moves period"/>
    <decimal name="priceOutStockMovePeriod" readonly="true" title="Amount out. moves period"/>
    <string name="label" readonly="true"/>
  </entity>
</domain-models>
